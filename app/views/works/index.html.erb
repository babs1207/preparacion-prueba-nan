<p id="notice"><%= notice %></p>
<h1>Association</h1>

<table class="table">
<thead>
    <tr>
      <th>People</th>
      <th>Project</th>
      <th colspan="3"></th>
    </tr>
  </thead>


  <tbody>
    <tr>
     <% @association.each do |association| %>
      <td><%= association.person.name %></td>
      <td><%= association.project.name%></td>
      <td><%= link_to 'Destroy', association, method: :delete, data: { confirm: '¿Estás seguro?' } %></td>
    </tr>
    <% end %>
  </tbody>
</table>